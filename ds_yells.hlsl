//Note: Because these color-coded yells are character-specific,
//their yell SFX are included in their functions. As such, it
//is unneccessary to include the SFX in the game script.

//Damion Shard
[pw_raiseholditdamion]
loadandplaysfx("../damionshard/phoenix-holdit.wav",0);
setvar([pwdoingyell],1);
setgenobjgraphic("pw_yell",12);
setgenobjsizefromtexture("pw_yell");
showgenobj("pw_yell");
placeshake("pw_yell","genobj",60,20,19);
wait(60);
hidegenobj("pw_yell");
setvar([pwdoingyell],0);

